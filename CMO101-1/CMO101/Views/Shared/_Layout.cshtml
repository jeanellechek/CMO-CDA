<!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <link rel="apple-touch-icon" sizes="76x76" href="../assets/img/apple-icon.png" />
    <link rel="icon" type="image/png" href="../assets/img/favicon.png" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <title>Crisis Management Office</title>
    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport' />
    <meta name="viewport" content="width=device-width" />
    <!-- Bootstrap core CSS     -->
    @Styles.Render("~/Content/styles/bootstrap.min.css")
    <!--  Material Dashboard CSS    -->
    @Styles.Render("~/Content/styles/material-dashboard.css")
    <!--     Fonts and icons     -->
    @Styles.Render("http://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css")
    @Styles.Render("http://fonts.googleapis.com/css?family=Roboto:400,700,300|Material+Icons")
    <!--   Core JS Files   -->
    @Scripts.Render("~/Content/scripts/jquery-3.2.1.min.js")
    @Scripts.Render("~/Content/scripts/bootstrap.min.js")
    @Scripts.Render("~/Content/Scripts/material.min.js")
    <!--  Dynamic Elements plugin -->
    @Scripts.Render("~/Content/Scripts/arrive.min.js")
    <!--  PerfectScrollbar Library -->
    @Scripts.Render("~/Content/Scripts/perfect-scrollbar.jquery.min.js")
    <!--  Notifications Plugin    -->
    @Scripts.Render("~/Content/Scripts/bootstrap-notify.js")

    <!-- Material Dashboard javascript methods -->
    @Scripts.Render("~/Content/Scripts/material-dashboard.js")
    <script src="~/Content/Scripts/demo.js"></script>

    @RenderSection("scripts", required: false)
    <style>
        #map {
            height: 350px;
            width: 100%;
        }
    </style>
</head>

<body>
    @RenderBody();
</body>






</html>